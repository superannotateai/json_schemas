{
  "title": "VideoAnnotation",
  "type": "object",
  "properties": {
    "metadata": {
      "$ref": "#/definitions/MetaData"
    },
    "instances": {
      "title": "Instances",
      "default": [],
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "#/definitions/EventInstance"
          },
          {
            "$ref": "#/definitions/BboxInstance"
          }
        ]
      }
    },
    "tags": {
      "title": "Tags",
      "default": [],
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag"
      }
    }
  },
  "required": [
    "metadata"
  ],
  "definitions": {
    "LastUserAction": {
      "title": "LastUserAction",
      "type": "object",
      "properties": {
        "email": {
          "title": "Email",
          "type": "string",
          "format": "email"
        },
        "timestamp": {
          "title": "Timestamp",
          "type": "integer"
        }
      },
      "required": [
        "email",
        "timestamp"
      ]
    },
    "AnnotationStatusEnum": {
      "title": "AnnotationStatusEnum",
      "description": "An enumeration.",
      "enum": [
        "NotStarted",
        "InProgress",
        "QualityCheck",
        "Returned",
        "Completed",
        "Skipped"
      ],
      "type": "string"
    },
    "MetaData": {
      "title": "MetaData",
      "type": "object",
      "properties": {
        "url": {
          "title": "Url",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "lastAction": {
          "$ref": "#/definitions/LastUserAction"
        },
        "width": {
          "title": "Width",
          "type": "integer"
        },
        "height": {
          "title": "Height",
          "type": "integer"
        },
        "projectId": {
          "title": "Projectid",
          "type": "integer"
        },
        "annotatorEmail": {
          "title": "Annotatoremail",
          "type": "string",
          "format": "email"
        },
        "qaEmail": {
          "title": "Qaemail",
          "type": "string",
          "format": "email"
        },
        "status": {
          "$ref": "#/definitions/AnnotationStatusEnum"
        },
        "duration": {
          "title": "Duration",
          "type": "integer"
        }
      }
    },
    "ImageRoleEnum": {
      "title": "ImageRoleEnum",
      "description": "An enumeration.",
      "enum": [
        "Customer"
      ],
      "type": "string"
    },
    "UserAction": {
      "title": "UserAction",
      "type": "object",
      "properties": {
        "email": {
          "title": "Email",
          "type": "string",
          "format": "email"
        },
        "role": {
          "$ref": "#/definitions/ImageRoleEnum"
        }
      },
      "required": [
        "email",
        "role"
      ]
    },
    "CreationTypeEnum": {
      "title": "CreationTypeEnum",
      "description": "An enumeration.",
      "enum": [
        "Manual",
        "Prediction",
        "Pre-annotation"
      ],
      "type": "string"
    },
    "VideoType": {
      "title": "VideoType",
      "description": "An enumeration.",
      "enum": [
        "event",
        "bbox"
      ],
      "type": "string"
    },
    "Attribute": {
      "title": "Attribute",
      "type": "object",
      "properties": {
        "id": {
          "title": "Id",
          "type": "integer"
        },
        "groupId": {
          "title": "Groupid",
          "type": "integer"
        },
        "name": {
          "title": "Name",
          "minLength": 1,
          "type": "string"
        },
        "groupName": {
          "title": "Groupname",
          "minLength": 1,
          "type": "string"
        }
      },
      "required": [
        "groupName"
      ]
    },
    "BaseTimeStamp": {
      "title": "BaseTimeStamp",
      "type": "object",
      "properties": {
        "active": {
          "title": "Active",
          "type": "boolean"
        },
        "attributes": {
          "title": "Attributes",
          "type": "object",
          "patternProperties": {
            "^[-|+]$": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Attribute"
              }
            }
          }
        }
      }
    },
    "EventInstance": {
      "title": "EventInstance",
      "type": "object",
      "properties": {
        "createdAt": {
          "title": "Createdat",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d{3})Z",
          "type": "string"
        },
        "updatedAt": {
          "title": "Updatedat",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d{3})Z",
          "type": "string"
        },
        "createdBy": {
          "$ref": "#/definitions/UserAction"
        },
        "updatedBy": {
          "$ref": "#/definitions/UserAction"
        },
        "creationType": {
          "default": "Pre-annotation",
          "allOf": [
            {
              "$ref": "#/definitions/CreationTypeEnum"
            }
          ]
        },
        "classId": {
          "title": "Classid",
          "type": "integer"
        },
        "className": {
          "title": "Classname",
          "minLength": 1,
          "type": "string"
        },
        "id": {
          "title": "Id",
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/VideoType"
        },
        "locked": {
          "title": "Locked",
          "type": "boolean"
        },
        "timeline": {
          "title": "Timeline",
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/BaseTimeStamp"
          }
        }
      },
      "required": [
        "type",
        "timeline"
      ]
    },
    "BboxPoints": {
      "title": "BboxPoints",
      "type": "object",
      "properties": {
        "x1": {
          "title": "X1",
          "type": "number"
        },
        "x2": {
          "title": "X2",
          "type": "number"
        },
        "y1": {
          "title": "Y1",
          "type": "number"
        },
        "y2": {
          "title": "Y2",
          "type": "number"
        }
      },
      "required": [
        "x1",
        "x2",
        "y1",
        "y2"
      ]
    },
    "BboxTimeStamp": {
      "title": "BboxTimeStamp",
      "type": "object",
      "properties": {
        "active": {
          "title": "Active",
          "type": "boolean"
        },
        "attributes": {
          "title": "Attributes",
          "type": "object",
          "patternProperties": {
            "^[-|+]$": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Attribute"
              }
            }
          }
        },
        "points": {
          "$ref": "#/definitions/BboxPoints"
        }
      }
    },
    "PointLabels": {
      "title": "PointLabels",
      "type": "object",
      "patternProperties": {
        "^[0-9]+$": {
          "type": "string"
        }
      }
    },
    "BboxInstance": {
      "title": "BboxInstance",
      "type": "object",
      "properties": {
        "createdAt": {
          "title": "Createdat",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d{3})Z",
          "type": "string"
        },
        "updatedAt": {
          "title": "Updatedat",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d{3})Z",
          "type": "string"
        },
        "createdBy": {
          "$ref": "#/definitions/UserAction"
        },
        "updatedBy": {
          "$ref": "#/definitions/UserAction"
        },
        "creationType": {
          "default": "Pre-annotation",
          "allOf": [
            {
              "$ref": "#/definitions/CreationTypeEnum"
            }
          ]
        },
        "classId": {
          "title": "Classid",
          "type": "integer"
        },
        "className": {
          "title": "Classname",
          "minLength": 1,
          "type": "string"
        },
        "id": {
          "title": "Id",
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/VideoType"
        },
        "locked": {
          "title": "Locked",
          "type": "boolean"
        },
        "timeline": {
          "title": "Timeline",
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/BboxTimeStamp"
          }
        },
        "pointLabels": {
          "$ref": "#/definitions/PointLabels"
        }
      },
      "required": [
        "type",
        "timeline"
      ]
    },
    "Tag": {
      "title": "Tag",
      "minLength": 1,
      "type": "string"
    }
  }
}