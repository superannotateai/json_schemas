{
  "title": "PixelAnnotation",
  "type": "object",
  "properties": {
    "metadata": {
      "$ref": "#/definitions/PixelMetaData"
    },
    "instances": {
      "title": "Instances",
      "type": "array",
      "items": {
        "$ref": "#/definitions/PixelAnnotationInstance"
      }
    },
    "tags": {
      "title": "Tags",
      "default": [],
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag"
      }
    }
  },
  "required": [
    "metadata",
    "instances"
  ],
  "definitions": {
    "LastUserAction": {
      "title": "LastUserAction",
      "type": "object",
      "properties": {
        "email": {
          "title": "Email",
          "type": "string",
          "format": "email"
        },
        "timestamp": {
          "title": "Timestamp",
          "type": "integer"
        }
      },
      "required": [
        "email",
        "timestamp"
      ]
    },
    "AnnotationStatusEnum": {
      "title": "AnnotationStatusEnum",
      "description": "An enumeration.",
      "enum": [
        "NotStarted",
        "InProgress",
        "QualityCheck",
        "Returned",
        "Completed",
        "Skipped"
      ],
      "type": "string"
    },
    "PixelMetaData": {
      "title": "PixelMetaData",
      "type": "object",
      "properties": {
        "url": {
          "title": "Url",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "minLength": 1,
          "type": "string"
        },
        "lastAction": {
          "$ref": "#/definitions/LastUserAction"
        },
        "width": {
          "title": "Width",
          "type": "integer"
        },
        "height": {
          "title": "Height",
          "type": "integer"
        },
        "projectId": {
          "title": "Projectid",
          "type": "integer"
        },
        "annotatorEmail": {
          "title": "Annotatoremail",
          "type": "string",
          "format": "email"
        },
        "qaEmail": {
          "title": "Qaemail",
          "type": "string",
          "format": "email"
        },
        "status": {
          "$ref": "#/definitions/AnnotationStatusEnum"
        },
        "isSegmented": {
          "title": "Issegmented",
          "type": "boolean"
        }
      },
      "required": [
        "name"
      ]
    },
    "ImageRoleEnum": {
      "title": "ImageRoleEnum",
      "description": "An enumeration.",
      "enum": [
        "Customer"
      ],
      "type": "string"
    },
    "UserAction": {
      "title": "UserAction",
      "type": "object",
      "properties": {
        "email": {
          "title": "Email",
          "type": "string",
          "format": "email"
        },
        "role": {
          "$ref": "#/definitions/ImageRoleEnum"
        }
      },
      "required": [
        "email",
        "role"
      ]
    },
    "CreationTypeEnum": {
      "title": "CreationTypeEnum",
      "description": "An enumeration.",
      "enum": [
        "Manual",
        "Prediction",
        "Pre-annotation"
      ],
      "type": "string"
    },
    "Attribute": {
      "title": "Attribute",
      "type": "object",
      "properties": {
        "id": {
          "title": "Id",
          "type": "integer"
        },
        "groupId": {
          "title": "Groupid",
          "type": "integer"
        },
        "name": {
          "title": "Name",
          "minLength": 1,
          "type": "string"
        },
        "groupName": {
          "title": "Groupname",
          "minLength": 1,
          "type": "string"
        }
      },
      "required": [
        "groupName"
      ]
    },
    "PixelColor": {
      "title": "PixelColor",
      "anyOf": [
        {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            },
            {
              "type": "integer"
            }
          ]
        },
        {
          "type": "array",
          "items": [
            {
              "type": "integer"
            },
            {
              "type": "integer"
            },
            {
              "type": "integer"
            },
            {
              "type": "number"
            }
          ]
        },
        {
          "type": "string"
        }
      ]
    },
    "PixelAnnotationPart": {
      "title": "PixelAnnotationPart",
      "type": "object",
      "properties": {
        "color": {
          "$ref": "#/definitions/PixelColor"
        }
      },
      "required": [
        "color"
      ]
    },
    "PixelAnnotationInstance": {
      "title": "PixelAnnotationInstance",
      "type": "object",
      "properties": {
        "createdAt": {
          "title": "Createdat",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d{3})Z",
          "type": "string"
        },
        "updatedAt": {
          "title": "Updatedat",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d{3})Z",
          "type": "string"
        },
        "createdBy": {
          "$ref": "#/definitions/UserAction"
        },
        "updatedBy": {
          "$ref": "#/definitions/UserAction"
        },
        "creationType": {
          "default": "Pre-annotation",
          "allOf": [
            {
              "$ref": "#/definitions/CreationTypeEnum"
            }
          ]
        },
        "classId": {
          "title": "Classid",
          "type": "integer"
        },
        "className": {
          "title": "Classname",
          "minLength": 1,
          "type": "string"
        },
        "visible": {
          "title": "Visible",
          "type": "boolean"
        },
        "locked": {
          "title": "Locked",
          "type": "boolean"
        },
        "probability": {
          "title": "Probability",
          "default": 100,
          "type": "integer"
        },
        "attributes": {
          "title": "Attributes",
          "default": [],
          "type": "array",
          "items": {
            "$ref": "#/definitions/Attribute"
          }
        },
        "error": {
          "title": "Error",
          "type": "boolean"
        },
        "parts": {
          "title": "Parts",
          "type": "array",
          "items": {
            "$ref": "#/definitions/PixelAnnotationPart"
          }
        }
      },
      "required": [
        "parts"
      ]
    },
    "Tag": {
      "title": "Tag",
      "minLength": 1,
      "type": "string"
    }
  }
}